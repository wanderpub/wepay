{
    "type": "library",
    "name": "wander/wepay",
    "homepage": "https://github.com/wanderpub/wepay.git",
    "description": "微信支付v3版 for PHP,支持微信支付v3版商户直连模式（WePay）和服务商模式（WePayPartner）",
    "license": "MIT",
    "authors": [
        {
            "name": "wander",
            "email": "13834563@qq.com"
        }
    ],
    "keywords": [
        "WePay",
        "WePayV3",
        "WePayPartner",
        "WeChatPay",
        "WeXinPay"
    ],
    "require": {
        "php": ">=7.1",
        "ext-xml": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-bcmath": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "WePay\\": "WePay",
            "WePayPartner\\": "WePayPartner"
        }
    }
}
